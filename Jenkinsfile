#!/usr/bin/env groovy
#!groovy
import groovy.time.*

    node{
    

    options{
    timestamps()
    }
    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
        stage('Testing') {
            steps {
                echo 'Testing'
            }
        }
    }
 }

